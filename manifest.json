{
  "manifest_version": 2,
  "name": "FirefoxMe",
  "version": "0.0.1",
  "description": "Manage my world",
  "author": "KK",
  "permissions": [
    "<all_urls>",
    "tabs",
    "tabGroups",
    "webRequest"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "type": "module"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_title": "FirefoxMe"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
